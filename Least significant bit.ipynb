   "source": [
    "a=int(input())\n",
    "b=input()\n",
    "    \n",
    "def LSB(num): \n",
    "    return bool(num & (1 << 0 )) \n",
    "  \n",
    " \n",
    "count=0\n",
    "for i in range(0,len(b),2):\n",
    "    res=LSB(int(b[i]))\n",
    "    if res==True:\n",
    "        count+=1\n",
    "        \n",
    "print(count)"
   ]
