   "source": [
    "def sum(list):\n",
    "    sum=0\n",
    "    output=[]\n",
    "    for i in range(len(list)):\n",
    "        sum=sum+list[i]\n",
    "        output.append(sum)\n",
    "        \n",
    "    return output\n",
    "\n",
    "list=[10,10,20,30,40]\n",
    "print(list)\n",
    "sum(list)\n",
    "        "
   ]
